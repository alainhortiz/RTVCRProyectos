                            $(this).css("background-color", "#ECF8E0");
                        });
                        //Aqui ya tenemos los valores de la fila
                        contratadosTCP.push({
                            carnetIdentidad: filaCI,
                            noContrato: filaNoContrato,
                            servicio: filaServicio,
                            presupuestoCargo: filaPresupuestoCargo,
                            totalPagar: filaTotalPagar,
                            valorPrograma: filaValorPrograma
                        });
                    });
                }

                $(".preload").removeClass('hidden');

                var mat_datos = {
                    noContratoCanal: $("#addContratoCanal").val(),
                    nombreDirector: $("#addNombreDirector").val(),
                    nombreProductor: $("#addNombreProductor").val(),
                    titulo: $("#addTitulo").val(),
                    formato: $("#addFormatoProyecto").val(),
                    cantEmisiones: $("#addEmisiones").val(),
                    tiempoPantalla: $("#addTiempoPantalla").val(),
                    categoria: $("#addTipoProyecto").val(),
                    fechaInicio: $("#addFechaInicio").val(),
                    fechaFinal: $("#addFechaFinal").val(),
                    preFilmacion: $("#addPreFilmacion").val(),
                    filmacion: $("#addFilmacion").val(),
                    postFilmacion: $("#addPostFilmacion").val(),
                    descripcion: $("#addDescripcion").val(),
                    contratados: contratados,
                    contratadosTCP: contratadosTCP
                };

                $.ajax({
                    type: "POST",
                    dataType: "html",
                    url: '{{ path("insertProyecto") }}',
                    data: mat_datos
                }).done(function (data) {

                    $(".preload").addClass('hidden');

                    if (data === 'ok') {

                        alertify.alert('<strong>El proyecto ha sido insertado correctamente</strong>')
                            .setHeader('<span class="glyphicon glyphicon-ok-circle" style="font-size: 20px"></span>' +
                                '<h4 class="modal-title" style="display: inline-block; margin-left: 10px; color: white">Confirmación</h4>')
                            .set('label', 'Aceptar')
                            .set('onok', function () {
                                window.location.href = "{{ path('gestionarProyectos') }}";
                            });
                    } else {
                        alertify.alert('<strong>' + data + '</strong>')
                            .setHeader('<span class="glyphicon glyphicon-ban-circle" style="font-size: 20px"></span>' +
                                '<h4 class="modal-title" style="display: inline-block; margin-left: 10px">Error</h4>')
                            .set('label', 'Aceptar')
                    }
                });

            });

        });
    </script>
{% endblock %}

